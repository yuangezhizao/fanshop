<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width,initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta content="black" name="apple-mobile-web-app-status-bar-style" />
    <meta content="telephone=no" name="format-detection" />
    <meta content="email=no" name="format-detection" />
    <meta charset="UTF-8">
    <title>惠家有购物商城</title>
    <link href="../css/base.css" rel="stylesheet">
    <link href="../css/ch-box.css" rel="stylesheet">
    <script src="../js/jquery-2.1.4.js"></script>
    <script src="../js/g_header.js"></script>
</head>
<body>
    <h1 class="back-hd" id="d_go_top" title="惠家有·秒杀" module="202088"><a class="go-back" id="btnBack"></a>惠家有·秒杀</h1>
    <section class="main">
        <div class="title"></div>
        <div id="div_main">

        </div>
        <div class="titlefoot"></div>
    </section>
    <script type="text/template" id="tpl_seckill_time">
        <h2 class="{{h2class}}">{{timetype}}</h2>
        <ul module="202089">
            {{productList}}
        </ul>
    </script>
    <script type="text/template" id="tpl_seckill_time_product">
        <li>
            <a onclick="page_seckill.ShowDetail('{{productlink}}')">
                <font id="{{fontid}}"></font>
                <b><img src="{{picture}}" alt=""><span style="{{isSoldOut_style}}" id="{{spanid}}"><i>已抢光</i></span></b>
                <dl>
                    <dt>{{pname}}</dt>
                    <dd>
                        <font>
                            {{ccurpricelist}}
                        </font>
                        <span><i style="{{timeleftpercent_style}}"></i></span>
                        <div class="shu">剩余{{salesVolume}}件</div>
                        <label id="{{labelid}}">
                            {{ccurtimelist}}
                        </label>
                    </dd>
                </dl>
            </a>
        </li>
    </script>

    <script type="text/javascript">
        var staticlist = [[], ["/js/functions/g_Const.js", "/js/functions/g_Type.js", "/js/tost.js", "/js/pages/group.js", "/js/pages/Seckill.js", "/js/g_footer.js"]];
        WriteStatic(staticlist);
    </script>
    <script>
        page_seckill.event_product_sortType = g_const_event_product_sortType.SaleCount;
        page_seckill.Init();
        page_seckill.Refresh();
    </script>
    <script src="../js/g_footer.js"></script>
</body>
</html>
<script type="text/javascript">
    //测试记录用户行为
    (function () {
        window.page = "102005";
        var gas = document.createElement("script");
        gas.type = "text/javascript";
        gas.async = true;
        gas.src = "http://" + window.location.host + "/js/gas.js";

        var l = document.getElementsByTagName("script").length;
        var g = document.getElementsByTagName("script")[l - 1];
        g.parentNode.insertBefore(gas, g);
    })();
</script>